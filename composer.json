{
    "name": "qpautrat/woohoolabs-yin-bundle",
    "description": "Implements woohoolabs/yin framework into Symfony",
    "keywords": ["json-api"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Quentin Pautrat",
            "email": "quentin.pautrat@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/psr-http-message-bridge": "^0.2.0",
        "zendframework/zend-diactoros": "^1.3",
        "woohoolabs/yin": "^0.10.2"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.11",
        "phpspec/phpspec": "^2.5",
        "sensio/framework-extra-bundle": "^3.0"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "Allows you to use ParamConverter and access to JsonApi instance from your controllers"
    },
    "autoload": {
        "psr-4": {
            "QPautrat\\WoohoolabsYinBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\QPautrat\\WoohoolabsYinBundle\\": "tests/app"
        }
    }
}
