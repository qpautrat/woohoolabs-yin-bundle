{
    "name": "qpautrat/woohoolabs-yin-bundle",
    "description": "Implements woohoolabs/yin framework into Symfony",
    "keywords": ["json-api", "symfony"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Quentin Pautrat",
            "email": "quentin.pautrat@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/psr-http-message-bridge": "^1.0",
        "zendframework/zend-diactoros": "^1.3",
        "woohoolabs/yin": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.1",
        "phpspec/phpspec": "^3.2",
        "sensio/framework-extra-bundle": "^3.0"
    },
    "suggest": {
        "sensio/framework-extra-bundle": "Allows you to use ParamConverter and access to JsonApi instance from your controllers"
    },
    "autoload": {
        "psr-4": {
            "QP\\WoohoolabsYinBundle\\": "src/"
        }
    }
}
